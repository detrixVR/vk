<table data-toggle="bootgrid"
       data-ajax="true"
       data-url="grid"
       data-list-type="process"
       data-selection="true"
       data-multi-select="true"
       data-keep-selection="true"
       class="table table-condensed table-hover table-striped"
       id="processGrid">
    <thead>
    <tr>
        <th data-column-id="_id"
            data-visible="false"
            data-identifier="true"
            data-visible-in-selection="false"
            data-type="string">ID
        </th>

        <th data-column-id="created"
            data-order="1"
            data-visible="false"
            data-visible-in-selection="false">Created
        </th>

        <th data-formatter="title"
            data-column-id="processId">
            Название
        </th>

        <th data-column-id="status"
            data-formatter="status"
            data-sortable="true"
            data-width="200px">Статус
        </th>

        <th data-formatter="commandsAll"
            data-sortable="false"
            data-width="100px"
            data-visible-in-selection="false"></th>
    </tr>
    </thead>
</table>