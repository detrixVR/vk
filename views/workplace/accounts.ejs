<!DOCTYPE html>
<html lang="en">
<head>
    <%- include ../parts/cabinetHead.ejs %>
</head>
<body>


<% var processId='validateAccounts'%>

<%- include ../parts/cabinetMenu.ejs %>

<div class="canvas">

    <%- include ../templates/overlay.ejs %>

    <%- include ../parts/cabinetNavbar.ejs %>

    <div class="container-fluid">
        <div class="row">
            <div class="col-xs-12">
                <div class="well well-sm container-fluid">
                    <div class="row">

                        <div class="col-xs-6 col-sm-3 col-md-2">
                            <label class="small control-label" data-toggle="tooltip" data-placement="top" title="" data-original-title="Какой список использовать">Проверять</label>
                            <select class="selectpicker" data-width="100%" id="targetSelect" data-toggle="tooltip" data-placement="left" title="" data-original-title="Tooltip on left">
                                <option>Все</option>
                                <option>Загруженные</option>
                                <option>Выделенные</option>
                            </select>
                        </div>



                        <div class="col-xs-6 col-sm-3 col-md-2">
                            <label class="small control-label"></label>

                            <div class="checkbox">
                                <input type="checkbox" id="deleteIfWrong" data-settings="true">
                                <label for="deleteIfWrong">
                                    Удалять невалидные
                                </label>
                            </div>
                        </div>

                        <div class="col-xs-6 col-sm-3 col-md-2">
                            <label class="small control-label"></label>
                            <div class="checkbox">
                                <input type="checkbox" id="checkGetUserInfo" checked/>
                                <label for="checkGetUserInfo">
                                    Получать информацию
                                </label>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-12 col-lg-8">
                <div class="well well-sm">
                    <%- include ../templates/accountGrid.ejs %>
                </div>
            </div>
            <div class="col-xs-12  col-lg-4">
                <div class="well well-sm widget eventsHolder">
                    <div class="bootgrid-header">
                        <%- include ../parts/buttonsProcess.ejs %>
                    </div>
                    <div class="listHolder">
                        <ul class="list-group">
                        </ul>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>

<%- include ../templates/modalInput.ejs %>


<script src="javascripts/libs.js"></script>
<script src="javascripts/app.js"></script>

</body>
</html>